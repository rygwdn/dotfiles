#!/usr/bin/env python3
# encoding: utf-8

import sys
from subprocess import check_output, CalledProcessError
import re

reflog_match = re.compile(r".*moving from (.*) to (.*)")

reflog = check_output("git reflog", shell=True).decode("utf-8").splitlines()
recent = []
for line in reflog:
    match = reflog_match.match(line)
    if match:
        recent.append(match.group(1).strip())
        recent.append(match.group(2).strip())

all_heads = check_output(
    "git for-each-ref refs/heads --format='%(refname)'",
    shell=True).decode('utf-8').splitlines()

try:
    current_branch = check_output("git symbolic-ref HEAD 2>/dev/null", shell=True).decode('utf-8').strip()
except CalledProcessError:
    current_branch = ""

branches = []
for head in all_heads:
    if head == current_branch:
        continue
    branches.append(head[len("refs/heads/"):])

missing_branches = [b for b in branches if b not in recent]

seen = set()
sorted_recent = []
for branch in recent + missing_branches:
    if branch in seen:
        continue
    seen.add(branch)
    if branch not in branches:
        continue
    sorted_recent.append(branch)

print("\n".join(sorted_recent))
