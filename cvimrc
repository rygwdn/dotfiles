let homedir='/Users/rwooden'
let configpath='~/.cvimrc'
set localconfig

set numerichints
set typelinkhints
set noautofocus
set nativelinkorder
let barposition = "bottom"
map b :buffer<Space>
map s :open?<Space>
map S :open!<Space>
map d closeTab
map R reloadTabUncached
map D :script if(document.getSelection){s=document.getSelection();}else{s='';};document.location='https://pinboard.in/add?next=same&url='+encodeURIComponent(location.href)+'&description='+encodeURIComponent(s)+'&title='+encodeURIComponent(document.title)<CR>
map u lastClosedTab
map <C-d> scrollPageDown
map <C-u> scrollPageUp
let searchengine aj = "http://jira.aio-tv.com/secure/QuickSearch.jspa?searchString=%s"
let completionengines = ["google", "aj"]
site '*://jira.aio-tv.com/*' {
   unmap .
}
